{
   "presets": ["next/babel"],
   "plugins": [
      [
         "styled-components",
         {
            "ssr": true,
            "displayName": true,
            "preprocess": false
         }
      ],
      [
         "module-resolver",
         {
            "root": ["./"],
            "alias": {
               "Components": "./components",
               "Data": "./data",
               "Utils": "./utils",
               "Service": "./service"
            }
         }
      ]
   ]
}
